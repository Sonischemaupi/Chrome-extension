{
  "manifest_version": 3,
  "name": "LocalStorage Online Modifier",
  "version": "3.5",
  "description": "Sla localStorage online op en deel het.",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["https://api.jsonbin.io/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Upload/Download localStorage data"
  }
}
